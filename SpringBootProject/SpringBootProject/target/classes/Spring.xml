<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->


    <bean id="Car" class="org.genspark.Car" >
        <!-- <property name="ty" ref="Tyre"/> -->
        <property name="d" ref="Id"/>
        <property name="nm" ref="Name"/>
       <property name="mob" ref="Phone"/>
       <property name="ad" ref="Address"/>
    </bean>






  <!--  <bean id="Tyre" class="org.genspark.Tyre"> -->
  <!--    <property name="brand" value="MRF"/> -->
  <!--      <property name="size" value="17"/> -->
  <!--  </bean> -->

    <bean id="Phone" class="org.genspark.Phone">
        <property name="mob" value="mobstr"/>
    </bean>

    <bean id="Address" class="org.genspark.Address">
        <property name="city" value="detroit"/>
        <property name="state" value="mi"/>
        <property name="country" value="usa"/>
        <property name="zipcode" value="1234"/>

    </bean>


    <bean id="Name" class="org.genspark.Name">
        <property name="namestr" value="yoda"/>
    </bean>

    <bean id="Id" class="org.genspark.Id">
        <property name="id" value="12"/>
    </bean>


     </beans>